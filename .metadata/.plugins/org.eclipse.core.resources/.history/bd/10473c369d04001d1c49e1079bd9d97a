	int multi, num1, num2; 
	
	num1 = sc.nextInt();
	num2 = sc.nextInt();
	
	multi = num1 * num2;
	
	System.out.printf("Soma de %d e %d Ã© %d%n", num1, num2, multi);
	
	}

}
